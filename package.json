    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "del": "^2.2.0",
    "eslint": "^3.15.0",
    "eslint-config-defaults": "^9.0.0",
    "node-sprite-generator": "^0.10.2"
  },
